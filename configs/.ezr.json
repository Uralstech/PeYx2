{
    "metadata":
        {
            "version":"1.1.0",
            "web-version":"https://pastebin.com/raw/iSuqm6fj",
            "download":"https://github.com/Uralstech/PeYx2/tree/master/Builds",
            "author":"Udayshankar Ravikumar",
            "language":"ezr"
        },
    
    "command":"ezrShell ___FILE___",
    "syntaxes":
        [
            {
                "name":"keywords",
                "regex":"\\b(IF|ELSE|DO|COUNT|FROM|AS|TO|STEP|WHILE|FUNCTION|WITH|END|TRY|ERROR|OBJECT|INCLUDE)\\b",
                "color":"#a80adc"
            },
            {
                "name":"keywords-light",
                "regex":"\\b(GLOBAL|ITEM|AND|OR|IN|INVERT|RETURN|SKIP|STOP|NOTHING|TRUE|FALSE|EZR_VERSION)\\b",
                "color":"#0080ff"
            },
            {
                "name":"comment",
                "regex":"@.*",
                "color":"#008000"
            },
            {
                "name":"number",
                "regex":"\\b[1234567890]*\\b",
                "color":"#b9c0ff"
            },
            {
                "name":"special-chars",
                "regex":"(\\+|-|\\*|\\/|\\^|%|!|=|<|>)",
                "color":"#8080ff"
            },
            {
                "name":"string",
                "regex":"([\"'])((\\\\{2})*|(.*?[^\\\\](\\\\{2})*))\\1",
                "color":"#ffa264",

                "sub-syntaxes":
                [
                    {
                        "name":"string-escape",
                        "regex":"(\\\\'|\\\\\"|\\\\t|\\\\n|\\\\r|\\\\{2})",
                        "color":"#ff8000"
                    }
                ]
            }
        ]
}